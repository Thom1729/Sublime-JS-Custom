---
contexts:
  object-property-name:
    - meta_prepend: true
    - match: '{{identifier_name}}'
      scope: string.unquoted.js
      pop: true

  variable-binding-object-key:
    - meta_prepend: true
    - match: '{{identifier_name}}(?=\s*:)'
      scope: string.unquoted.js
      pop: true
